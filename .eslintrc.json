{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-newline": "warn",
    "newline-after-var": "warn",
    "newline-before-return": "warn",
    "newline-per-chained-call": "warn"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
